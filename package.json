{
  "type": "module",
  "name": "fe-workspace",
  "version": "1.0.0",
  "description": "前端工程效率平台工作空间",
  "main": "index.js",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "publish": "node scripts/publish-packages.cjs",
    "publish:webpack-spec-plugin": "node scripts/publish-packages.cjs webpack-spec-plugin",
    "publish:minor": "node scripts/publish-packages.cjs \"\" minor",
    "publish:major": "node scripts/publish-packages.cjs \"\" major",
    "build": "pnpm -r --stream build",
    "test": "pnpm -r --stream test",
    "dev": "pnpm -r --stream --parallel dev"
  },
  "keywords": [
    "frontend",
    "monorepo",
    "pnpm",
    "vitepress",
    "documentation"
  ],
  "author": "chuanjing.li",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "favicons": "^7.2.0",
    "vitepress": "^1.0.0"
  }
}